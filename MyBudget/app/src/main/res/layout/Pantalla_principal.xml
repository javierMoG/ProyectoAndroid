<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gabriel.mybudget.PantallaPrincipal">

    <!-- TODO: Update blank fragment layout -->


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/BtnConsultaWeb"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/BtnIngresarGasto"
        android:layout_centerHorizontal="true"
        android:text="Precio del dolar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:id="@+id/BtnIngresarGasto"
        android:text="Ingresar Gasto"
        android:layout_below="@+id/BtnEstablecerIM"
        android:layout_alignRight="@+id/BtnConsultaWeb"
        android:layout_alignEnd="@+id/BtnConsultaWeb" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/BtnEstablecerIM"
        android:layout_marginTop="67dp"
        android:text="Establecer Ingreso "
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
